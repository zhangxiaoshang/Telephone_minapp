/* pages/check/check.wxss */
.container {
  display: flex;
  flex-direction: column;
  margin: 0 35px;
  font-size: 32rpx;
}

.container .tip {
  text-align: center;
  color: #999;
  font-size: 16px;
  margin-top: 100px;
}

.container .item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #eee;
  color: #666;
}
